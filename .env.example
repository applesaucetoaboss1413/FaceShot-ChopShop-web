# FaceShot-ChopShop Backend Environment Variables

# Server Configuration
NODE_ENV=production
PORT=3000

# Database
DB_PATH=production.db

# JWT Authentication
SESSION_SECRET=your-super-secret-jwt-key-change-this-in-production

# A2E API Configuration
A2E_API_KEY=your-a2e-api-key-here
A2E_BASE_URL=https://api.a2e.ai

# Stripe Configuration
STRIPE_SECRET_KEY=your-stripe-secret-key-here
STRIPE_WEBHOOK_SECRET=your-stripe-webhook-secret-here

# Cloudinary Configuration
CLOUDINARY_CLOUD_NAME=your-cloudinary-cloud-name
CLOUDINARY_API_KEY=your-cloudinary-api-key
CLOUDINARY_API_SECRET=your-cloudinary-api-secret

# Pricing Configuration
COST_PER_CREDIT=0.0111
MIN_MARGIN=0.40

# Frontend URL (for CORS and redirects)
FRONTEND_URL=http://localhost:8080

# Admin Access (comma-separated email list)
ADMIN_EMAILS=admin@example.com

# Logging
LOG_LEVEL=info

# Optional: Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Optional: Job Processing
MAX_JOB_SECONDS=5000
